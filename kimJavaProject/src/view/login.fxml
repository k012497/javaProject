            <Label fx:id="lblFindPw" text="FIND PASSWORD">
               <font>
                  <Font name="NanumSquareOTF Regular" size="12.0" />
               </font></Label>
            <Separator orientation="VERTICAL" prefHeight="20.0" style="-fx-background-color: #dcdcdc;" />
            <ImageView fx:id="imgAdmin" fitHeight="29.0" fitWidth="28.0">
               <image>
                  <Image url="@../images/setting.png" />
               </image>
            </ImageView>
         </children>
      </HBox>
      <PasswordField fx:id="txtPw" layoutX="39.0" layoutY="345.0" prefHeight="39.0" prefWidth="274.0" promptText="Password">
         <font>
            <Font name="NanumSquareOTF Regular" size="13.0" />
         </font></PasswordField>
   </children>
</AnchorPane>
